<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Besoinlogistique" table="besoinlogistique">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomDemendeur" type="string" column="nom_demendeur" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="numDemendeur" type="string" column="num_demendeur" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateB" type="date" column="date_b" nullable="false"/>
    <field name="siteDep" type="string" column="site_dep" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="siteDes" type="string" column="site_des" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="etat" type="string" column="etat" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
